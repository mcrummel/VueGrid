{
    "name": "vue-grid",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/mcrummel/VueGrid"
    },
    "bugs": {
        "url": "https://github.com/mcrummel/VueGrid/issues"
    },
    "author": {
        "name": "Michael Crummel"
    },
    "type": "module",
    "files": ["dist"],
    "main": "./dist/vue-grid.umd.cjs",
    "module": "./dist/vue-grid.js",
    "exports": {
        ".": {
            "import": "./dist/vue-grid.js",
            "require": "./dist/vue-grid.umd.cjs"
        },
        "./dist/style.css": "./dist/style.css",
        "./dist/public/loading.gif": "./dist/public/loading.gif"
    },
    "types": "./dist/vue-grid.d.ts",
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "lint": "eslint . --ignore-path .eslintignore --ext .js,.ts,.vue",
        "dev": "vite",
        "build": "vite build && vue-tsc -p tsconfig.json --emitDeclarationOnly",
        "types": "vue-tsc",
        "preview": "vite preview"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
        "@fortawesome/free-brands-svg-icons": "^6.4.2",
        "@fortawesome/free-regular-svg-icons": "^6.4.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "@fortawesome/vue-fontawesome": "^3.0.5",
        "vue": "^3.3.9"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "eslint": "^8.54.0",
        "eslint-plugin-vue": "^9.19.2",
        "sass": "^1.69.3",
        "sass-loader": "^10.4.1",
        "typescript": "^5.3.2",
        "vite": "^4.4.5",
        "vite-plugin-dts": "^3.6.4",
        "vue": "^3.3.9",
        "vue-router": "^4.0.13",
        "vue-tsc": "^1.8.24"
    },
    "keywords": [
        "vue 3",
        "vue-component",
        "table"
    ]
}
